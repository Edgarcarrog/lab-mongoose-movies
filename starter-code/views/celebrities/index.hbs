<h2>Celebrities:</h2>
<ul>
  {{#each celebrities}}
    <li>
      <a href="/celebrities/{{_id}}">{{name}}</a>
      <form action="/celebrities/{{_id}}/delete" method="POST">
      <button type="submit">Delete</button>
      </form>
    </li>
  {{/each}}
</ul>
<h3>Create a new celebrity</h3>
<a href="/celebrities/new">Accept</a> 